        <api_name>x_58872_needit.GetEmailAddr</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>return email from user record (sys_id)</description>
        <name>GetEmailAddr</name>
        <script><![CDATA[var GetEmailAddr = Class.create();
GetEmailAddr.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	getEmail: function(){
		var userRec = new GlideRecord("sys_user");
		userRec.get(this.getParameter('sysparm_userID'));
		return userRec.email + '';
	},
    type: 'GetEmailAddr'
});]]></script>
